<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/imageView5" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="72dp"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="59dp"
            android:layout_width="61dp"
            android:id="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="40dp"
            motion:layout_constraintHorizontal_bias="0.482" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="178dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView3"
            motion:layout_constraintTop_toTopOf="@+id/imageView2"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView2"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="175dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView4"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:layout_height="69dp"
            android:layout_width="80dp"
            android:id="@+id/imageView5"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.871"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="69dp"
            android:layout_width="67dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="160dp"
            motion:layout_constraintHorizontal_bias="0.889" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="72dp"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="73dp"
            android:layout_width="74dp"
            android:id="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            android:layout_marginTop="68dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="73dp"
            android:layout_width="77dp"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            android:layout_marginLeft="76dp"
            android:layout_marginStart="76dp" />
        <Constraint
            android:layout_height="70dp"
            android:layout_width="72dp"
            android:id="@+id/imageView5"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="28dp"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
</MotionScene>